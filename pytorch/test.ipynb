   "source": [
    "# import datasets\n",
    "import torch\n",
    "from torchtext.datasets import IMDB\n",
    "\n",
    "train_iter = IMDB(split='train')\n",
    "\n",
    "def tokenize(label, line):\n",
    "    return line.split()\n",
    "\n",
    "tokens = []\n",
    "for i, [label,line] in enumerate(train_iter):\n",
    "    if i > 500:\n",
    "        break\n",
    "    tokens += tokenize(label, line)"
   ]
  }
